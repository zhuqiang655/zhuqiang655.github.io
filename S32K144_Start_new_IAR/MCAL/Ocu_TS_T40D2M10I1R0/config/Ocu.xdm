                            <!-- @implements OcuConfigSet_Object -->
                            <v:ctr name="OcuConfigSet" type="IDENTIFIABLE">
                                <a:a name="DESC">
                                    <a:v><![CDATA[EN:
                                    <html>
                                         This container is the base of a Configuration Set, which contains the configured OCU channels. This way, different configuration sets can be defined for post-build process.
                                    </html> ]]>
                                    </a:v>
                                </a:a>
                                <a:a name="UUID" value="ECUC:5ce51b69-fffb-4b87-863d-7463e09eeeb1"/>
                            
