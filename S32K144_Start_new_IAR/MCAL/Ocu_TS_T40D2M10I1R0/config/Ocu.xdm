                                            <icc:v mclass="PreCompile">VariantPostBuild</icc:v>
                                            <icc:v mclass="PreCompile">VariantPreCompile</icc:v>
                                        </a:a>
                                         
                                        <!-- @implements OcuChannelId_Object -->    
                                        <v:var name="OcuChannelId" type="INTEGER">
                                            <a:a name="DESC">
                                                <a:v><![CDATA[EN:
                                                <html>
                                                Channel Id of the OCU channel. This value will be assigned to the symbolic name derived from the OcuChannel container short name. It defines the assignment of the channel to the physical OCU hardware channel.
                                                </html> ]]>
                                                </a:v>
                                            </a:a>                                                
                                            <a:a name="IMPLEMENTATIONCONFIGCLASS"  type="IMPLEMENTATIONCONFIGCLASS">
                                                <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                                <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                            </a:a>
                                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                            <a:a name="SCOPE" value="LOCAL"/>
                                            <a:a name="SYMBOLICNAMEVALUE" value="true"/>
                                            <a:a name="UUID" value="ECUC:ae7f2b9b-74ab-4a31-9d76-7c4eca6f36ac"/>
                                            <a:da name="INVALID" type="Range">
                                                <a:tst expr="&lt;=65535"/>
                                                <a:tst expr="&gt;=0"/>
                                            </a:da>
                                            <a:da name="DEFAULT" type="XPath" expr="node:fallback(node:current()/../@index, num:i(1))"/>
                                            <a:da name="INVALID" type="XPath">
                                                <a:tst expr="(node:value(.) >= num:i(count(../../*/.)))" 
                                                true="Ocu channel IDs is out of range maximum value is given by the total number of channels."/>
                                                <a:tst expr="text:uniq(../../*/OcuChannelId, .)"                                   
                                                false="Ocu channels should have unique IDs."/>                              
                                            </a:da>                                            
                                        </v:var> 
                                        
                                        <!-- @implements OcuAssignedHardwareChannel_Object -->
                                        <v:var name="OcuAssignedHardwareChannel" type="INTEGER">
                                            <a:a name="DESC">
                                                <a:v><![CDATA[EN:
                                                <html>
                                                The physical hardware channel that is assigned to this logical channel.
                                                </html> ]]>
                                                </a:v>
                                            </a:a>
                                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>                                            
                                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                            </a:a>
                                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                            <a:a name="SCOPE" value="LOCAL"/>
                                            <a:a name="READONLY" value="true"/>
                                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                            <a:a name="UUID" value="ECUC:41b79bdf-db86-4a56-b6f6-ce1a3dcd3491"/>                                    
                                            <a:da name="DEFAULT" type="XPath" expr="node:fallback(node:current()/../@index, num:i(1))"/>
                                            <a:da name="INVALID" type="Range">
                                                <a:tst expr="&lt;=255"/>
                                                <a:tst expr="&gt;=0"/>
                                            </a:da>
                                        </v:var>
                                        
                                        <v:var name="FtmHwChannel" type="ENUMERATION">
                                            <a:a name="DESC">
                                                <a:v><![CDATA[EN:
                                                <html>
                                                The Ftm hardware channel that is assigned to this logical channel.
                                                </html> ]]>
                                                </a:v>
                                            </a:a>
                                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                            </a:a>
                                            <a:a name="ORIGIN" value="NXP"/>
                                            <a:a name="SCOPE" value="LOCAL"/>
                                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                            <a:a name="UUID" value="ECUC:41b79bdf-dc86-4a56-b6f6-ce1a3dcd3491"/>
                                            <a:da name="INVALID" type="XPath">
                                                <a:tst expr="text:uniq(../../*/FtmHwChannel, .)" false="Duplicate physical Ftm module"/>
                                                <a:tst expr="node:exists(../../../../OcuGeneral/OcuHwResourceConfig/*[contains(./OcuHwResourceId , node:current()) and ./OcuChannelIsUsed = 'true'])" false="Please enable this channel in OcuHwResourceConfig/*/OcuChannelIsUsed"/>
                                                <a:tst expr="node:exists(../../../OcuHWSpecificSettings/*[text:split(OcuFtmModule,'_')[2] = text:split(node:current()/../FtmHwChannel,'_')[2]])" false="This FTM module is not configured yet. Please add and configure corresponding FTM module in OcuHWSpecificSettings"/>
                                            </a:da>

                                            <a:da name="DEFAULT" type="XPath" expr="(ecu:list('Ocu.Ftm_Hw_Channels_List'))[position()-1=node:fallback(node:current()/../@index,'0')]">      
                                            </a:da>
                                            <a:da name="RANGE" type="XPath" expr="ecu:list('Ocu.Ftm_Hw_Channels_List')"/>    
                                        </v:var>
                               
                                        <!-- @implements OcuDefaultThreshold_Object -->
                                        <v:var name="OcuDefaultThreshold" type="INTEGER">
                                            <a:a name="DESC">
                                                <a:v><![CDATA[EN:
                                                <html>
                                                Value of comparison threshold used for Initialization.
                                                </html> ]]>
                                                </a:v>
                                            </a:a>
                                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/>                                            
                                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
