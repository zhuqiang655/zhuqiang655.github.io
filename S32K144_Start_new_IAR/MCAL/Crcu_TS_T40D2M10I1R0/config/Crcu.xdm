                                                  <a:tst expr="&lt;=255"/>
                                              </a:da>
                                              <a:da name="RANGE" type="XPath">
                                                  <a:tst expr="(. &gt;= 0) and (.&lt; num:i(count(node:current()/../../*)))"
                                                          false="Value out of range: must be in range 0 to N-1 (N is number of configured channel configuration). Use the Calc button to calculate correct default value"/>
                                                  <a:tst expr="text:uniq(../../*/CrcuChannelConfigId, .)"
                                                          false="Duplicated value, CrcuChannelConfigId must be unique across all Crcu channel configurations. Use the Calc button to calculate correct default value."/>
                                              </a:da>
                                            </v:var>

                                            <v:var name="Crcu_Width" type="ENUMERATION">
                                                <a:a name="LABEL" value="CRC Width"/>
                                                <a:a name="DESC">
                                                    <a:v>
                                                        <![CDATA[EN:
                                                            <html><p>
                                                             Width of the CRC protocol to be used. (16, 32 bit)
                                                            </p></html>
                                                        ]]>
                                                    </a:v>
                                                </a:a>
