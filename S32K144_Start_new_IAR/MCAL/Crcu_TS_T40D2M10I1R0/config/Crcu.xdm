                                            </a:a>
                                            <a:a name="UUID" value="ECUC:49154b03-0601-4bfd-8f96-5ec46a6543ea"/>
                                          
                                            <v:var name="CrcuChannelConfigId" type="INTEGER">
                                              <a:a name="DESC">
                                                  <a:v>
                                                      <![CDATA[EN:
                                                          <html>
                                                          Identifies the Crcu channel configuration. <p/>
                                                          Note: Implementation Specific Parameter. <p/>
                                                          </html>
