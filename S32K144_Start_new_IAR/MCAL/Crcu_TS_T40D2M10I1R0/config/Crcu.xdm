                            <!--  @implements CrcuConfig_Object  -->
                                <v:ctr name="CrcuConfig" type="IDENTIFIABLE">
                                    <a:a name="DESC" 
                                         value="EN: This container contains the configuration parameters and sub containers of the AUTOSAR Crcu module."/>
                                    <a:a name="UUID" value="ECUC:5e1b9d37-58b0-46ba-a549-c40c8e80d6f3"/>

                                    <!--  @implements CrcuChannel_Object  -->
                                    <v:lst name="CrcuChannel" type="MAP">
                                        <a:da name="MIN" value="1"/>
                                        <a:da name="MAX" value="255"/>
                                        <v:ctr name="CrcuChannel" type="IDENTIFIABLE">
                                            <a:a name="DESC">
                                                <a:v>
                                                    <![CDATA[EN:
                                                        <html><p>
                                                            Configuration of an individual Crcu channel. Symbolic names will be generated for each channel.
                                                        </p></html>
                                                    ]]>
                                                </a:v>
                                            </a:a>
                                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                <icc:v mclass="PreCompile">VariantPostBuild</icc:v>
                                                <icc:v mclass="PreCompile">VariantPreCompile</icc:v>
                                            </a:a>
                                            <a:a name="UUID" value="ECUC:37aaf24e-fe6b-4d85-9655-e20239723ac8"/>
                                            
                                            <v:var name="CrcuChannelId" type="INTEGER">
                                              <a:a name="DESC">
                                                  <a:v>
                                                      <![CDATA[EN:
                                                          <html>
                                                          Identifies the Crcu channel. <p/>
                                                          Note: Implementation Specific Parameter. <p/>
                                                          </html>
                                                      ]]>
                                                  </a:v>
                                              </a:a> 
                                              <a:a name="LABEL" value="Crcu Channel ID" />
                                              <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                                <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                              </a:a>
                                              <a:a name="ORIGIN" value="NXP"/>
                                              <a:a name="SCOPE" value="LOCAL"/>
                                              <a:a name="SYMBOLICNAMEVALUE" value="true"/>
                                              <a:a name="UUID" 
                                                   value="ECUC:aac952ef-7cb7-4c94-acc5-b6c4ca845e7a"/>
                                              <a:da name="DEFAULT" type="XPath" expr="node:fallback(node:current()/../@index, num:i(1))"/>
                                              <a:da name="INVALID" type="Range">
                                                  <a:tst expr="&gt;=0"/>
                                                  <a:tst expr="&lt;=255"/>
                                              </a:da>
                                              <a:da name="RANGE" type="XPath">
                                                  <a:tst expr="(. &gt;= 0) and (.&lt; num:i(count(node:current()/../../*)))"
                                                          false="Value out of range: must be in range 0 to N-1 (N is number of configured channels). Use the Calc button to calculate correct default value"/>
                                                  <a:tst expr="text:uniq(../../*/CrcuChannelId, .)"
                                                          false="Duplicated value, CrcuChannelId must be unique across all Crcu channels. Use the Calc button to calculate correct default value."/>
                                              </a:da>
                                            </v:var>
                                    
                                            <v:var name="CrcChannel" type="ENUMERATION">
                                                <a:a name="LABEL" value="CRC Channel"/>
                                                <a:a name="DESC">
                                                    <a:v>
                                                        <![CDATA[EN:
                                                            <html><p>
                                                                 Selects one of the CRC channels available on the platform.
                                                            </p></html>
                                                        ]]>
                                                    </a:v>
                                                </a:a>
                                                <a:a name="SCOPE" value="LOCAL"/>
                                                <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                </a:a>
                                                <a:a name="ORIGIN" value="NXP"/>
                                                <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                <a:a name="UUID" value="ECUC:0233ad95-d800-476d-a551-96fcf5c84481"/>
                                                <a:da name="DEFAULT" type="XPath" expr="(ecu:list('Crcu.CrcHwChannels'))[position()-1=node:fallback(node:current()/../@index, '0')]"/>
                                                <a:da name="RANGE" type="XPath" expr="ecu:list('Crcu.CrcHwChannels')"/>
                                                <a:da name="INVALID" type="XPath">
                                                    <a:tst expr="text:uniq(../../*/CrcChannel, .)" false="Duplicate physical CRC channel"/>
                                                </a:da>
                                            </v:var>
                                            <v:ref name="CrcuDmaChannelRef" type="REFERENCE">
                                                <a:a name="DESC">
                                                    <a:v>
                                                      <![CDATA[EN: 
                                                       <html>
                                                          <p>Reference to a DMA channel (set in the Mcl driver configuration) used by the Crcu driver when calling function Crcu_AsyncCalculateCrc().</p>
                                                          Note: Implementation Specific Parameter. <p/>
                                                      </html>
                                                      ]]>
                                                  </a:v>
                                                </a:a>
                                                <a:a name="SCOPE" value="LOCAL"/>
                                                <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                                    <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                                </a:a>
                                                <a:a name="OPTIONAL" value="true"/>
                                                <a:a name="LABEL" value="Crcu DMA Channel"/>
                                                <a:a name="ORIGIN" value="NXP"/>
                                                <a:a name="UUID" value="ECUC:5419ebbb-b73b-4ee2-9e88-22e201491b61"/>
                                                <a:a name="EDITABLE" type="XPath">
                                                  <a:tst expr="(node:fallback(../../../../CrcuGeneral/CrcuDmaUsed, 'false') = 'true')"/>
                                                </a:a>
                                                <a:a name="INVALID" type="XPath">
                                                  <a:tst expr="not(node:refvalid(.))" true="The selected DMA channel is not valid."/>
                                                  <a:tst expr="text:uniq(../../*/CrcuDmaChannelRef, .)" false="Duplicate DMA channel"/>
                                                </a:a>
                                                <a:da name="REF" value="ASPathDataOfSchema:/TS_T40D2M10I1R0/Mcl/MclConfigSet/DMAChannel"/>
                                            </v:ref> 
                                            <v:var name="CrcuAsyncCalculateCrcCompleteNotification" type="FUNCTION-NAME">
                                                <a:a name="LABEL" value="Async CRC Complete Notification"/>
                                                <a:a name="DESC">
                                                  <a:v>
                                                      <![CDATA[EN:<html>
                                                          Function name of notification called when an async CRC calculation operation has completed.<p/>
                                                          Note: Implementation Specific Parameter.
                                                      </html>]]>
                                                  </a:v>
                                                </a:a>
                                                <a:a name="SCOPE" value="LOCAL"/>
                                                <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                                        type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                </a:a>
                                                <a:a name="ORIGIN" value="NXP"/>
                                                <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                <a:a name="UUID" value="ECUC:6c3f6592-4a40-49c7-9ef9-1002f4024b09"/>
                                                <a:da name="DEFAULT" value="Crcu_AsyncCalculateCrcCompleteNotif"/>
                                                <a:a name="EDITABLE" type="XPath">
                                                    <a:tst expr="(node:fallback(../../../../CrcuGeneral/CrcuDmaUsed, 'false') = 'true' and node:exists(../CrcuDmaChannelRef))"/>
                                                </a:a>                                    
                                                <a:da name="INVALID" type="XPath">
                                                  <a:tst expr="(  (node:fallback(., '') = 'NULL') or
                                                                  (node:fallback(., '') = 'Null') or
                                                                  (node:fallback(., '') = 'false') or
                                                                  (node:fallback(., '') = 'FALSE') or
                                                                  (node:fallback(., '') = 'null') or
                                                                  (node:fallback(., '') = 'null_ptr') or
                                                                  (node:fallback(., '') = 'Null_Ptr') or
                                                                  (node:fallback(., '') = 'NULL_PTR') 
                                                               )"
                                                      true="Invalid name, it must be a valid C function name."/>
                                                  <a:tst expr="(normalize-space(node:fallback(., '')) = '&quot;NULL_PTR&quot;') or
                                                               (normalize-space(node:fallback(., '')) = 'NULL_PTR') or
                                                               (normalize-space(node:fallback(., '')) = '&quot;NULL&quot;') or
                                                               (normalize-space(node:fallback(., '')) = 'NULL') or
                                                               (text:match(normalize-space(.),'^[_a-zA-Z]+[_0-9a-zA-Z]*$'))"
                                                     false="Invalid name, it must be a valid C function name"/>
                                                </a:da>
                                            </v:var>
                                        </v:ctr>
                                    </v:lst>
                                    <!--  @implements CrcuChannelConfig_Object  -->
